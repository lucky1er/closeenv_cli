{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app-server",
    "types": [
      "node"
    ],
    "paths": {
      "@asymmetrik/ngx-leaflet": ["./src/app/stubs/leaflet.module"]
    },
    "target": "es2016"
},
  "files": [
    "src/main.server.ts",
    "server.ts"
  ],
  "angularCompilerOptions": {
    "entryModule": "./src/app/app.server.module#AppServerModule",
    "enableIvy": true
  }
}
